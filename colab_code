# Working Directory 설정 [Google Drive]
import os
os.chdir('/content/drive/MyDrive/Crypto/data/')

# BTC 저가 데이터[일간] import
import csv
import numpy as np
import matplotlib.pyplot as plt

data = []

dataReader = csv.reader(open('./BTC_low_price.csv', 'r'))
datalist = list(dataReader)
for row in datalist :
    for num in row :
        data.append(float(num))

plt.plot(data)
